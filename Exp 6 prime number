#include<stdio.h>
int main()
{
	int num, i, flag=0;
	printf("Enter the value of num :");
	scanf("%d",&num);
	
	if (num==0||num==1)
	
	flag=1;

	for(i=2;i<num/2;++i){
	
	if (num%2==0)
	{
		flag=1;
		break;
	}
	
    }
    if(flag==0)
    printf("%d is an prime number",num);
    else
    printf("%d is not an prime number",num);
    
    return 0;
}
